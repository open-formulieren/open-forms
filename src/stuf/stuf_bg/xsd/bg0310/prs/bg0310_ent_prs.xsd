	<complexType name="VES-antwoord-prs-r" final="">
		<annotation>
			<documentation>Restrictie op Vestiging, zoveel mogelijk overbodige elementen voor pefill
 verwijderd. </documentation>
		</annotation>
		<complexContent>
			<restriction base="BG:VES-basis">
				<sequence>
					<element name="vestigingsNummer" type="BG:VestigingsNummer-r" nillable="false"/>
					<sequence maxOccurs="unbounded">
						<element name="handelsnaam" type="BG:Handelsnaam-r" nillable="false"/>
						<element name="handelsnaamVerkort" type="BG:HandelsnaamVerkort-e" nillable="true"/>
					</sequence>
					<choice>
						<element name="verblijfsadres" type="BG:VerblijfsadresGrpVes-prs-r"/>
						<element name="sub.verblijfBuitenland" type="BG:VerblijfBuitenlandGrp" minOccurs="0"/>
					</choice>
					<element name="sub.correspondentieAdres" type="BG:CorrespondentieAdresGrp-prs-r" minOccurs="0"/>
					<element name="sub.telefoonnummer" type="BG:Telefoonnummer-e" nillable="true" minOccurs="0"/>
					<element name="sub.faxnummer" type="BG:Telefoonnummer-e" nillable="true" minOccurs="0"/>
					<element name="sub.emailadres" type="BG:Emailadres-e" nillable="true" minOccurs="0"/>
					<element name="sub.url" type="BG:Url-e" nillable="true" minOccurs="0"/>
					<element ref="StUF:extraElementen" minOccurs="0"/>
				</sequence>
				<attribute ref="StUF:entiteittype" use="required" fixed="VES"/>
				<attribute ref="StUF:sleutelVerzendend" use="optional"/>
				<attribute ref="StUF:sleutelOntvangend" use="prohibited"/>
				<attribute ref="StUF:sleutelGegevensbeheer" use="prohibited"/>
				<attribute ref="StUF:noValue" use="optional"/>
				<attribute ref="StUF:scope" use="prohibited"/>
				<attribute ref="StUF:verwerkingssoort" use="prohibited"/>
			</restriction>
		</complexContent>
	</complexType>
