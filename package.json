{
  "name": "openforms",
  "version": "1.0.7",
  "description": "Open Forms",
  "main": "src/static/openforms/js/openforms.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "test": "echo 'no tests set up yet'; exit 1",
    "build": "NODE_ENV=production webpack",
    "builddev": "NODE_ENV=development webpack",
    "watch": "webpack -w",
    "start": "webpack -w",
    "makemessages-en": "formatjs extract 'src/openforms/js/**/*.js' --format src/openforms/js/i18n-formatter.js --out-file src/openforms/js/lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "makemessages-nl": "formatjs extract 'src/openforms/js/**/*.js' --format src/openforms/js/i18n-formatter.js --out-file src/openforms/js/lang/nl.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "makemessages": "npm run makemessages-nl && npm run makemessages-en",
    "compilemessages": "formatjs compile-folder --ast src/openforms/js/lang src/openforms/js/compiled-lang"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/open-formulieren/open-forms.git"
  },
  "author": "Maykin Media",
  "license": "UNLICENSED",
  "homepage": "https://maykinmedia.nl",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@rjsf/core": "^3.2.1",
    "@tinymce/tinymce-react": "^3.12.6",
    "bem.js": "^1.0.10",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "core-js": "^3.11.1",
    "flatpickr": "^4.6.9",
    "formiojs": "^4.13.0",
    "immer": "^9.0.6",
    "json-logic-js": "^2.0.1",
    "leaflet": "^1.7.1",
    "microscope-sass": "^1.0.4",
    "paper-css": "^0.4.1",
    "proj4leaflet": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-formio": "^4.3.0",
    "react-intl": "^5.20.7",
    "react-modal": "^3.14.3",
    "react-tabs": "^3.2.2",
    "react-use": "^17.2.4",
    "use-immer": "^0.5.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.13.13",
    "@formatjs/cli": "^4.2.30",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-formatjs": "^10.3.4",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^5.0.2",
    "cssnano": "^4.1.10",
    "cypress": "^9.3.1",
    "ejs-loader": "^0.5.0",
    "mini-css-extract-plugin": "^0.11.1",
    "postcss": "^8.0.8",
    "postcss-loader": "^4.0.1",
    "postcss-selector-lint": "^1.0.2",
    "sass": "^1.32.12",
    "sass-loader": "^10.1.1",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "yargs": "^17.3.1"
  }
}
